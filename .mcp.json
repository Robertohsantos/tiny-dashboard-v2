      "command": "node",
      "args": ["./node_modules/@igniter-js/mcp-server/dist/index.js"],
      "env": {
        "GOOGLE_API_KEY": "",
        "GITHUB_TOKEN": ""
      }
    },
    "browser": {
      "command": "./node_modules/.bin/mcp-server-playwright",
      "args": [],
