{
  "name": "saas-boilerplate",
  "version": "2.0.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "compile": "next experimental-compile",
    "dev:email": "email dev --dir src/content/mails --port 3001",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "vitest --watch",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --coverage --ui",
    "test:e2e": "playwright test",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "db:studio": "prisma studio",
    "db:migrate:dev": "prisma migrate dev",
    "db:generate": "prisma generate",
    "auth:generate": "npx @better-auth/cli generate --config src/core/providers/auth.ts",
    "auth:migrate": "npx @better-auth/cli migrate --config src/core/providers/auth.ts",
    "stripe:webhook": "stripe listen --forward-to localhost:3000/api/billing/webhook",
    "analyze-bundle": "ANALYZE=true npm run build",
    "migrate:hooks": "tsx scripts/migrate-to-validated-hooks.ts",
    "migrate:hooks:analyze": "tsx scripts/migrate-to-validated-hooks.ts --analyze",
    "migrate:hooks:dry-run": "tsx scripts/migrate-to-validated-hooks.ts --migrate --dry-run",
    "migrate:hooks:apply": "tsx scripts/migrate-to-validated-hooks.ts --migrate",
    "validation:monitor": "open http://localhost:3000/admin/validation-monitor",
    "validation:activate:local": "tsx scripts/activate-validation-local.ts",
    "validation:activate:check": "tsx scripts/activate-validation-local.ts --check-only",
    "validation:test:integration": "vitest run src/modules/dashboard/hooks/__tests__/use-validated-hooks.integration.test.tsx",
    "validation:rollout:staging": "tsx scripts/rollout-validation-staging.ts"
  },
